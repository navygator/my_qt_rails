<% provide(:title, @user.name) %>
<% f_user = @client.user("self") %>
<div class="row">
  <aside class="span4">
    <section>
      <h1>
        <%= image_tag f_user[:photo] %>
        <%= @user.name %>
      </h1>
    </section>
    <% f_user.friends.each do |friend| %>
      <%= debug(friend) %>
    <% end %>
  </aside>
  <aside class="span4 offset5">
    <%= debug(@client.user_requests) %>
  </aside>
</div>

